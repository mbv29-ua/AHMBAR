;INCLUDE "tile_properties.inc"
;
;SECTION "Tile Properties Table", ROM0
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;; TILE PROPERTIES LOOKUP TABLE
;;;; 256 bytes, one per tile ID (0x00-0xFF)
;;;; Each byte contains property flags
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;tile_properties_table::
;    ; 0x00-0x0F: Special tiles (empty, bullet, digits)
;    DB TILE_PROP_EMPTY      ; 0x00: Empty
;    DB TILE_PROP_EMPTY      ; 0x01
;    DB TILE_PROP_EMPTY      ; 0x02
;    DB TILE_PROP_EMPTY      ; 0x03
;    DB TILE_PROP_EMPTY      ; 0x04
;    DB TILE_PROP_EMPTY      ; 0x05
;    DB TILE_PROP_EMPTY      ; 0x06
;    DB TILE_PROP_EMPTY      ; 0x07
;    DB TILE_PROP_EMPTY      ; 0x08
;    DB TILE_PROP_EMPTY      ; 0x09: TILE_BULLET
;    DB TILE_PROP_EMPTY      ; 0x0A: TILE_DIGIT_0
;    DB TILE_PROP_EMPTY      ; 0x0B: TILE_DIGIT_1
;    DB TILE_PROP_EMPTY      ; 0x0C: TILE_DIGIT_2
;    DB TILE_PROP_EMPTY      ; 0x0D: TILE_DIGIT_3
;    DB TILE_PROP_EMPTY      ; 0x0E: TILE_DIGIT_4
;    DB TILE_PROP_EMPTY      ; 0x0F: TILE_DIGIT_5
;
;    ; 0x10-0x1F: Characters
;    DB TILE_PROP_EMPTY      ; 0x10: TILE_COWBOY
;    DB TILE_PROP_EMPTY      ; 0x11: TILE_GOAL
;    DB TILE_PROP_EMPTY      ; 0x12
;    DB TILE_PROP_EMPTY      ; 0x13
;    DB TILE_PROP_EMPTY      ; 0x14
;    DB TILE_PROP_EMPTY      ; 0x15
;    DB TILE_PROP_EMPTY      ; 0x16
;    DB TILE_PROP_EMPTY      ; 0x17
;    DB TILE_PROP_EMPTY      ; 0x18
;    DB TILE_PROP_EMPTY      ; 0x19
;    DB TILE_PROP_EMPTY      ; 0x1A
;    DB TILE_PROP_EMPTY      ; 0x1B
;    DB TILE_PROP_EMPTY      ; 0x1C
;    DB TILE_PROP_EMPTY      ; 0x1D
;    DB TILE_PROP_EMPTY      ; 0x1E
;    DB TILE_PROP_EMPTY      ; 0x1F
;
;    ; 0x20-0x7F: Reserved/empty
;    DS 96, TILE_PROP_EMPTY
;
;    ; 0x80-0x81: ONLY SOLID TILES (bricks/blocks)
;    DB TILE_PROP_SOLID      ; 0x80: Brick block (SOLID)
;    DB TILE_PROP_SOLID      ; 0x81: Brick block variant (SOLID)
;
;    ; 0x82-0x87: Empty (not solid)
;    DB TILE_PROP_EMPTY      ; 0x82
;    DB TILE_PROP_EMPTY      ; 0x83
;    DB TILE_PROP_EMPTY      ; 0x84
;    DB TILE_PROP_EMPTY      ; 0x85
;    DB TILE_PROP_EMPTY      ; 0x86
;    DB TILE_PROP_EMPTY      ; 0x87
;
;    ; 0x88-0x8B: Door tiles
;    DB TILE_PROP_DOOR       ; 0x88: Door top-left
;    DB TILE_PROP_DOOR       ; 0x89: Door bottom-left
;    DB TILE_PROP_DOOR       ; 0x8A: Door top-right
;    DB TILE_PROP_DOOR       ; 0x8B: Door bottom-right
;
;    ; 0x8C-0x8F: Reserved
;    DB TILE_PROP_EMPTY      ; 0x8C
;    DB TILE_PROP_EMPTY      ; 0x8D
;    DB TILE_PROP_EMPTY      ; 0x8E
;    DB TILE_PROP_EMPTY      ; 0x8F
;
;    ; 0x90-0x95: NOT solid (empty/decorative)
;    DB TILE_PROP_EMPTY      ; 0x90: Not solid
;    DB TILE_PROP_EMPTY      ; 0x91: Not solid
;    DB TILE_PROP_EMPTY      ; 0x92
;    DB TILE_PROP_EMPTY      ; 0x93
;    DB TILE_PROP_EMPTY      ; 0x94: Not solid
;    DB TILE_PROP_EMPTY      ; 0x95: Not solid
;
;    ; 0x96-0x9F: Reserved
;    DS 10, TILE_PROP_EMPTY
;
;    ; 0xA0: Empty/air tile (common in your maps)
;    DB TILE_PROP_EMPTY      ; 0xA0: Air/empty space
;
;    ; 0xA1-0xDA: Reserved
;    DS 58, TILE_PROP_EMPTY
;
;    ; 0xDB: Brick/solid fill tile (very common in maps - the visible brick blocks)
;    DB TILE_PROP_SOLID      ; 0xDB: Solid brick block
;
;    ; 0xDC-0xFF: Reserved
;    DS 36, TILE_PROP_EMPTY
;
;tile_properties_table_end::
